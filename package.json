    "@nestjs/platform-socket.io": "^10.3.7",
    "@nestjs/websockets": "^10.3.7",
    "@prisma/client": "^5.15.1",
    "bull": "^4.12.9",
    "cache-manager": "^5.6.1",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dockerode": "^4.0.2",
